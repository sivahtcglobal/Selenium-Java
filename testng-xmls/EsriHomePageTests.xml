<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Esri Homepage Test Suite" preserve-order="true"
	parallel="tests" thread-count="2">

	<test name='EsriHomePageAdobeAnalyticScriptsTest'>
		<parameter name="bType" value="chrome" />
		<classes>
			<class name="com.esri.tests.esri.EsriHomePageAdobeAnalyticsTest" />
		</classes>
	</test>

	<test name='EsriHomePageIndustriesMegamenuOnChrome'>
		<parameter name="bType" value="chrome" />
		<parameter name="fileName" value="esri/EsriHomePageIndustriesTestData.csv" />
		<parameter name="startRow" value="1" />
		<parameter name="endRow" value="1" />
		<classes>
			<class name="com.esri.tests.esri.EsriHomePageIndustriesMegamenuTest" />
		</classes>
	</test>

