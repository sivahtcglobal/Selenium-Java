<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Careers Job Search Test Suite" preserve-order="true">

	<test name='CareersjobSearchTestOnChrome'>
		<parameter name="bType" value="chrome" />	
		<parameter name="fileName"
			value="careers/CareersJobSearch.csv" />
		<parameter name="startRow" value="1" />
		<parameter name="endRow" value="1" />
		<classes>
			<class name="com.esri.tests.careers.jobsearch.CareersJobSearchTest" />
		</classes>
	</test>

	<listeners>
		<listener class-name="com.esri.qa.reporting.ExtentReporter" />
		<listener class-name="com.esri.qa.reporting.ReportListener" />
	</listeners>

</suite> 
